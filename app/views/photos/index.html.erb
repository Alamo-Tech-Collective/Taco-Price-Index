<h1>Photos</h1>

<table>
  <thead>
    <tr>
      <th>URL</th>
      <th>Taco</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.url %></td>
        <td><%= photo.taco_id %></td>
        <td>
          <%= link_to 'Show', photo %>
          <%= link_to 'Edit', edit_photo_path(photo) %>
          <%= link_to 'Destroy', photo, data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Photo', new_photo_path %>
